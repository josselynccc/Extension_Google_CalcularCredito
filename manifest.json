{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "0.0.4",
  "description":"Extensión para calcular el costo de tu crédito",
  "icons":{
    "16":"images/icon-16.png",
    "32":"images/icon-32.png",
    "48":"images/icon-48.png",
    "128":"images/icon-128.png"
  },
  "action": {
    "default_title": "Click Me",   
    "default_popup": "popup.html"  
  },
  "permissions": ["activeTab"],

  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "css": ["ed.css"],
      "js": ["ed.js"]
    }
  ]
}